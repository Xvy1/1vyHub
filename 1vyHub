-- Scripts:

local function AKOKCYV_fake_script() -- Combat.LocalScript 
	local script = Instance.new('LocalScript', Combat)

	--
	
	
	local CombatButton = script.Parent.Parent.Combat
	local CombatIcon = script.Parent.CombatIcon
	
	
	--
	local Open = false
	local Players = game:GetService("Players")
	local Player = Players.LocalPlayer
	
	local Mouse = Player:GetMouse()
	
	script.Parent.MouseButton1Click:Connect(function()
		if (Open == false) then
	
	
			CombatButton.TextColor3 = Color3.new(0.317647, 0.211765, 0.776471)
			CombatIcon.ImageColor3 = Color3.new(0.317647, 0.211765, 0.776471)
			CombatButton.Text = "       Combat                           <"
	        script.Parent.Parent.Parent.Parent.Frames.CombatMenu.Visible = true
			Open = true
		elseif (Open == true) then
	
	
			Open = false
			CombatButton.TextColor3 = Color3.new(0.835294, 0.835294, 0.835294)
			CombatIcon.ImageColor3 = Color3.new(0.862745, 0.862745, 0.862745)
			CombatButton.Text = "       Combat                           >"
			script.Parent.Parent.Parent.Parent.Frames.CombatMenu.Visible = false
	
	
		end
	end)
	
	local Button = script.Parent
	
	function MouseEnter()
		Button.Hover.Playing = true
	end
	
	function MouseLeave()
		Button.Hover.Playing = false
	end
	
	function MouseClick()
		Button.Click.Playing = true
	end
	
	Button.MouseEnter:connect(MouseEnter)
	Button.MouseLeave:connect(MouseLeave)
	Button.MouseButton1Click:Connect(MouseClick)
end
coroutine.wrap(AKOKCYV_fake_script)()
